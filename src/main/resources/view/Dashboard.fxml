<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="controller.DashboardController">
    <children>
        <TableView fx:id="bookTableView" layoutX="14.0" layoutY="40.0" prefHeight="200.0" prefWidth="550.0">
            <columns>
                <TableColumn fx:id="titleColumn" text="Title" />
                <TableColumn fx:id="authorColumn" text="Author" />
                <TableColumn fx:id="copiesColumn" text="Copies" />
                <TableColumn fx:id="priceColumn" text="Price (AUD)" />
            </columns>
        </TableView>
        <Button fx:id="logoutButton" layoutX="14.0" layoutY="260.0" text="Logout" onAction="#handleLogoutButtonAction" />
    </children>
</AnchorPane>
